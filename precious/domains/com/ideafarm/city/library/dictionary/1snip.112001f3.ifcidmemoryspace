#define ifcIDmEMORYsPACE_max          0x100

#define ifcIDmEMORYsPACE_APPLICATION  0x100
#define ifcIDmEMORYsPACE_RESERVEDmAX  0x0ff
#define ifcIDmEMORYsPACE_RESERVEDmIN  0x008
#define ifcIDmEMORYsPACE_BANGrELAY    0x007
#define ifcIDmEMORYsPACE_WATCH        0x006
#define ifcIDmEMORYsPACE_MEMBRANE     0x005
#define ifcIDmEMORYsPACE_GLASS2       0x004
#define ifcIDmEMORYsPACE_GLASS1       0x003
#define ifcIDmEMORYsPACE_CLOAK        0x002
#define ifcIDmEMORYsPACE_UNIVERSE     0x001

#define ifcIDmEMORYsPACE_min          0x002
//EXCLUDES UNIVERSE ; NO PROCESS RUNS IN THAT SPACE

// 101-fff            RESERVED FOR FUTURE APPLICATION USE
// 001                APPLICATIONS CODE                                : ALL FUNCTIONALITY GOES HERE UNLESS IT MUST GO ELSEWHERE
// 8-ff               RESERVED FOR FUTURE SYSTEM USE
// 7                  BANGrELAY                                        : BANG RELAY INTERHOME COMMUNICATION SUBSYSTEM
// 6                  WATCH (MAINTENANCE AND ANCILLARY DIAGNOSTIC)     : SELF TUNING AND LEARNING, ARCHIVAL, GLASS WALLS
// 5                  MEMBRANE ACCESSIBLE BY BOTH GLASS2 AND APP CODE  : NOT CURRENTLY USED; SIMPLE SMALL FUNCTIONAL SCOPE; MUST BE ROCK SOLID
// 4                  GLASS2                                           : ADAM THAT LAUNCHES ALL OTHER IPDOS (TM) PROCESSES AND CATCHES THE DEBUGGER EVENTS THAT THEY THROW ; ALSO USES booksC INSTANCES TO MONITOR TELEMETRY AND GENERATE MINUTE REPORTS OF THE CALL NEST OF EACH THREAD OF ALL IPDOS PROCESSES, INCLUDING SELF AND GLASS1 AND CLOAK AND UNIVERSE
// 3                  GLASS1                                           : SIMPLE ADAM THAT LAUNCHES GLASS2 AND CATCHES THE DEBUGGER EVENTS THAT IT THROWS
// 2                  CLOAK                                            : ANTI-MALWARE SYSTEM INTEGRITY MONITOR
// 1                  UNIVERSE                                         : USED ONLY WHEN PROCESSES RUNNING IN DIFFERENT MEMORY SPACES MUST ACCESS SHARED MEMORY (E.G. FOR SERIALIZATION ACROSS ALL PROCESSES IN A HOME)
//
//MEMORY SPACES

